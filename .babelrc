{
  "presets": [
    ["@babel/preset-env", {
      "modules": false
    }],
    "@babel/preset-react"
  ],
  "plugins": [
    ["@babel/plugin-transform-runtime", {
      "helpers": false
    }],
    "@babel/plugin-proposal-optional-chaining",
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-syntax-async-generators",
    "@babel/plugin-syntax-object-rest-spread"

  ]
}
